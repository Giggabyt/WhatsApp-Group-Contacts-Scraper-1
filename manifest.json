{
   "name": "WhatsApp Contacts Exporter by Codegena",
   "short_name": "WAXP",
   "version": "1.0",
   "manifest_version": 2,
   "description": "Export saved and unsaved contacts with names & status from WhatsApp groups for free.",
   "homepage_url": "https://codegena.com",
   "icons": { 
      "16": "/image/icon@16x16.png",
      "48": "/image/icon@48x48.png",
      "128": "/image/icon@128x128.png"
   },
   "browser_action": {
     "default_icon": "/image/icon@128x128.png",
     "default_title": "WAXP",
     "default_popup": "popup.html"
   },
   "content_scripts": [
      {
         "matches": ["*://web.whatsapp.com/"],
         "js": ["/js/WAXP.js"]
      }
   ],
   "background": {
      "scripts": ["/js/background.js"]
  },
   "update_url": "https://codegena.com/WhatsApp-Contacts-Exporter/updates.xml",
   "permissions" : ["activeTab"]
 }